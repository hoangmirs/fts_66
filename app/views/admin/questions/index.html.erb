<h1><%= t ".title" %></h1>
<div class="row">
  <div class="col-md-2">
    <%= link_to t(".new_question"), new_admin_question_path,
      remote: true, class: "btn btn-primary" %>
  </div>
  <%= render "admin/questions/search_form" %>
</div>
<%= tag :hr %>
<div class="row">
  <table class="table table-bordered table-striped">
    <thead>
      <tr class="info">
        <th><%= t ".content" %></th>
        <th><%= t ".subject" %></th>
        <th><%= t ".create_by" %></th>
        <th><%= t ".create_date" %></th>
        <th><%= t ".confirmed" %></th>
        <th><%= t ".actions" %></th>
      </tr>
    </thead>
    <tbody class="question-index">
      <%= render @questions %>
    </tbody>
  </table>
  <%= will_paginate @questions %>
  <div id="question-modal" class="modal fade"></div>
</div>
